{
  "name": "@nelsonlaidev/eslint-config",
  "version": "0.0.0",
  "homepage": "https://github.com/nelsonlaidev/config/packages/eslint-config/README.md",
  "bugs": {
    "url": "https://github.com/nelsonlaidev/config/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nelsonlaidev/config.git",
    "directory": "packages/eslint-config"
  },
  "license": "MIT",
  "author": "Nelson Lai <me@nelsonlai.me> (https://github.com/nelsonlaidev/)",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "build:inspector": "eslint-config-inspector build --config ./eslint-inspector.config.ts",
    "clean": "rm -rf .turbo dist",
    "dev": "tsdown --watch",
    "dev:inspector": "eslint-config-inspector --config eslint-inspector.config.ts",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint --fix .",
    "type-check": "tsc --noEmit"
  },
  "lint-staged": {
    "*.ts": "eslint --fix"
  },
  "dependencies": {
    "@eslint-community/eslint-plugin-eslint-comments": "catalog:prod",
    "@eslint-react/eslint-plugin": "catalog:prod",
    "@eslint/js": "catalog:prod",
    "@next/eslint-plugin-next": "catalog:prod",
    "@typescript-eslint/eslint-plugin": "catalog:prod",
    "@typescript-eslint/parser": "catalog:prod",
    "@vitest/eslint-plugin": "catalog:prod",
    "eslint-config-flat-gitignore": "catalog:prod",
    "eslint-config-prettier": "catalog:prod",
    "eslint-flat-config-utils": "catalog:prod",
    "eslint-plugin-better-tailwindcss": "catalog:prod",
    "eslint-plugin-command": "catalog:prod",
    "eslint-plugin-de-morgan": "catalog:prod",
    "eslint-plugin-import-lite": "catalog:prod",
    "eslint-plugin-jsdoc": "catalog:prod",
    "eslint-plugin-jsx-a11y": "catalog:prod",
    "eslint-plugin-n": "catalog:prod",
    "eslint-plugin-playwright": "catalog:prod",
    "eslint-plugin-prettier": "catalog:prod",
    "eslint-plugin-react-hooks": "catalog:prod",
    "eslint-plugin-react-refresh": "catalog:prod",
    "eslint-plugin-regexp": "catalog:prod",
    "eslint-plugin-simple-import-sort": "catalog:prod",
    "eslint-plugin-sonarjs": "catalog:prod",
    "eslint-plugin-unicorn": "catalog:prod",
    "eslint-plugin-unused-imports": "catalog:prod",
    "globals": "catalog:prod",
    "local-pkg": "catalog:prod"
  },
  "devDependencies": {
    "@eslint/config-inspector": "catalog:dev",
    "@nelsonlaidev/typescript-config": "workspace:*",
    "@types/node": "catalog:dev",
    "eslint": "catalog:dev",
    "tsdown": "catalog:dev"
  },
  "peerDependencies": {
    "eslint": ">=9.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
